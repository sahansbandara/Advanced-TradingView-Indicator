// Function to calculate MA based on type
get_ma(source, length, type) =>
    float ma = na
    if type == "SMA"
        ma := ta.sma(source, length)
    else if type == "EMA"
        ma := ta.ema(source, length)
    else if type == "WMA"
        ma := ta.wma(source, length)
    else if type == "RMA"
        ma := ta.rma(source, length)
    else if type == "VWMA"
        ma := ta.vwma(source, length)
    ma

// Calculate MAs
