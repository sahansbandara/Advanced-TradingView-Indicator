                            dw.style  := internal ? line.style_dashed : line.style_solid
                            choch      = brdw.get(0)
                            choch.x2  := time
                            choch.y   := ms.choch
                            choch.txt := "CHoCH"

                            
                                     
                        ms.xloc       := bar_index
                        
                        blarea.normal := false

            1 =>
