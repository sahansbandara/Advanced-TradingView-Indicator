ma2 = get_ma(close, ma2_len, ma_type)
ma3 = get_ma(close, ma3_len, ma_type)
ma4 = get_ma(close, ma4_len, ma_type)
ma5 = get_ma(close, ma5_len, ma_type)
ma6 = get_ma(close, ma6_len, ma_type)
ma7 = get_ma(close, ma7_len, ma_type)

